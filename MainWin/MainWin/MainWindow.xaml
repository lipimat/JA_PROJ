        <Button Content="RunDLLs" HorizontalAlignment="Left" Margin="365,250,0,0" VerticalAlignment="Top" Width="75" Click="onClickRunDLLs"/>
        <Label Content="Assembly function output" HorizontalAlignment="Left" Margin="185,165,0,0" VerticalAlignment="Top"/>
        <Label Content="C++ function output" HorizontalAlignment="Left" Margin="500,165,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
