        xmlns:local="clr-namespace:MainWin"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
		<TextBox x:Name="textBoxASM" HorizontalAlignment="Left" Height="23" Margin="200,202,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
